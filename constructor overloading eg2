package com.package1.project;

class Calc{
	int num1 = 1;
	int num2 = 2;
	int result;
	
	//constructor
	public Calc() {
		result = num1 + num2;
		System.out.println("Constructor Calc() will be called and it's ingredients will be get in place!");
	}
	
	//constructor overloading
	public Calc(int n) {
		System.out.println(n);
	}
	
	//constructor overloading
	public Calc(double f) {
		System.out.println(f);
	}
	
}


class RunnerClass{
	public static void main(String[] args) {
		Calc obj1 = new Calc(9.7); 
		//System.out.println(obj1.result); if u run this statement then u will get 0 as garbage value assigned to result.
	}
}




