package com.package1.project;

public class FirstClass {

	public static void main(String[] args) {
		
		int x = 1;
		int y = 2;
		
		//x = y;
		
		//y = x++; post increment (firsts increments the value of x and then assigns)
		//y = ++x; pre increment (firsts assigns the value of x to y and then increments the value of x)
		
		System.out.println(y);
	}

}
