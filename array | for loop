package com.package1.project;

public class ArrayDemo {

	public static void main(String[] args) {
		
		int arr1[] =  new int[4];    // int arr1[] = {1,2,3,4}
		
		arr1[0] = 1;
		arr1[1] = 2;
		arr1[2] = 3;
		arr1[3] = 4;
		//arr1[4] = 5;
		
		for(int n=0; n<4; n++) {
			System.out.println(arr1[n]);
		}
		

	}

}
